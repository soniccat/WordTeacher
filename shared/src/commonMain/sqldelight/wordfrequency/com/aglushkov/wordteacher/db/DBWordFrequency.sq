CREATE TABLE IF NOT EXISTS wiki (
  word TEXT,
  frequency REAL
);

selectOrderedFrequencyWithOffset:
SELECT frequency
FROM wiki
ORDER BY frequency DESC
LIMIT 1
OFFSET ?;

selectFrequencies:
SELECT *
FROM wiki
WHERE word IN ?;

selectFrequency:
SELECT frequency
FROM wiki
WHERE word = ?;

selectCount:
SELECT COUNT(*)
FROM wiki;