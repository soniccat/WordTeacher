CREATE TABLE IF NOT EXISTS DBArticle (
  id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
  name TEXT NOT NULL,
  date INTEGER NOT NULL
);

selectAll:
SELECT *
FROM DBArticle;

insert:
INSERT INTO DBArticle(name, date)
VALUES (?, ?);

insertArticle:
INSERT INTO DBArticle(name, date)
VALUES ?;

lastInsertedRowId:
SELECT last_insert_rowid();

removeAll:
DELETE
FROM DBArticle;
